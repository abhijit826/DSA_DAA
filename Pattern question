#include<iostream>
using namespace std;
int main(){
int i=1;
int n;
cin >> n;
int count =1;
while(i<=n){
	int j=1;
	while(j<=i){
		cout<<count<<"  "; // straight forward
          count =count+1;


	j=j+1;
	}
	cout<<endl;
	i=i+1; 
}
}



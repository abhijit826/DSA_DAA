Q>>>>>>>>>>
#include<iostream>
using namespace std;

int main(){
    int i=1;
    int n;
    std::cout <<"enter the numbner of n"  << std::endl;
    cin>>n;
    while(i<=n){
        int space=n-i;
        while(space){
            cout<<" ";
            space=space-1;
            
        }
        // print 
        int j=1;
        while(j<=i){
            cout<<j;
            j=j+1;
        }
        
        // printing the third triangle;
    
        int start =i-1;
        while(start){
            cout<<start;
            start=start-1;
        }
        cout<<endl;
        i=i+1;
    }
}

OUTPUT:>
enter the numbner of n
4
   1
  121
 12321
1234321


Q>>>??
#include <iostream>
using namespace std;

int main() {
  int n;
  int i=1;
  cin>>n;
  while(i<=n){
      int space=1;
      while(space<i){
          cout<<" ";
         space=space+1;
      }
         
      int j=1;
      while(j<=n-i+1){
          cout<<i;
          j=j+1;
      }
      cout<<endl;
      i=i+1;
  }
   
  
}


OUTPUT:
4
1111
 222
  33
   4




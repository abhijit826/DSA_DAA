#include<iostream>

using namespace std;
int main(){
int n;
cout<<"enter the Value of N :";
cin>> n;

 int i=1;
 while(i<=n){
    // space print karne ka 
    int space=n-i;
 while(space){
         cout<<" ";
   space =space-1;
   
 }
    // print stars;
   int j=1;
    while(j<=i){
               cout<<"*";
               j=j+1;
    }
     cout<<endl;
     i=i+1;
 }
}



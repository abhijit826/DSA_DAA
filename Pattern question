3 2 1 
4 3 2 1 
5 4 3 2 1 




#7 question
#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter the value of n: ";
    cin >> n;

 
    int i = 1;

    while (i <= n) {  // Loop for rows
        int j = 1;
        while (j <= n) {  // Loop for columns
           char ch = 'A'+i+j-2;  // Starting character(MAIN LOGIC)
            cout << ch<< " ";
            ch=ch+1;
            j=j+1;
        }
        cout << endl;
        i=i+1;
    }

    return 0;
}



>>>> 
OUTPUT:
A B C 
B C D 
C D E 



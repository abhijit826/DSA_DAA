#include<iostream>

using namespace std;
int main(){
int n;
cout<<"enter the Value of N :";
cin>> n;
char ch='A';
 int i=1;
 while(i<=n){
     int j=1;
     while(j<=i){
         cout<<ch<<" ";
         ch=ch+1;
         j=j+1;
     }
     cout<<endl;
     i=i+1;
 }
}

OUTPUT>>:
A 
B C 
D E F 
G H I J 




#9 question
#include<iostream>

using namespace std;
int main(){
int n;
cout<<"enter the Value of N :";
cin>> n;

 int i=1;
 while(i<=n){
     int j=1;
     while(j<=i){
         char ch=('A'+i+j-2);
         cout<<ch<<" ";
        //  ch=ch+1;
         j=j+1;
     }
     cout<<endl;
     i=i+1;
 }
}


OUTPUT:>>
enter the Value of N :4
A 
B C 
C D E 
D E F G 



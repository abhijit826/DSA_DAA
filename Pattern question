PRACTIVE
#include<iostream>
using namespace std;
int main(){
    int n;
    int i=1;
    cin>> n;
     int count=1;
    while(i<=n){
        int space=n-i;
        while(space){
            cout<<" ";
            space=space-1;
        }
        // int j=1;
       
        int j=1;
        while(j<=i){
            cout<<count;
            count=count+1;
            j=j+1;
        }
        cout<<endl;
        i=i+1;
    }
}
OUTPUT:
4
   1
  23
 456
78910





last pattern 
#include<iostream>
using namespace std;
int main(){
    int n;
    int i=1;
    cin>> n;
    //  int count=1;
    while(i<=n){
        int space=1;
        while(space<i){
            cout<<" ";
            space=space+1;
        }
        // int j=1;
       
        int j=i;
        while(j<=n){
            cout<<j;
            j=j+1;
        }
        cout<<endl;
        i=i+1;
    }
}

OUTPUT
1234 
 234
  34
   4

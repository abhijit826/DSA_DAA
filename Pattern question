#6 question
#include<iostream>
using namespace std;

int main(){
    int n;
    int i=1;
    int count=1;
    
    cin>> n;
   
    while(i<=n){
        int j=1;
        while( j<=i){
            cout<<i-j+1<<" ";
            // count=count+1;
          
       j=j+1;
        }
        cout<<endl;
        
         i=i+1;
    }
     
}

>>>>>>
OUTPUT:

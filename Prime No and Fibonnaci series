
// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
int n;
cout<<" enter the value of n";
cin>>n;
bool isPrime;
for(int i=2;i<=n;i++){
    if(n%i==0){
        isPrime=0;
        break;
    }
}
    if(isPrime==0){
        cout<<"not a prime"<<endl;
    }
    else{
    cout<<" it is a prime no";

}

   
    return 0;
}





// NOW TAKE A FIBONNACI SERIES;

// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
int n;
cout<<" enter the value of n";
cin>>n;
int a=0,b=1;
cout<<a <<b<<endl;
for(int i=0;i<=n;i++){
  int sum=a+b ;
  cout<<sum<<" ";
 a=b;
b=sum; 
}


   
    return 0;
}





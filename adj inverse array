// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
void changetheclor(int arr[],int n){
    for(int i=0;i<n-1;i+=2){
        swap(arr[i],arr[i+1]);
    }
    
}

int main() {
  int arr[]={4,5,6,8,10,2};
    int n;
    n=sizeof(arr)/sizeof(arr[0]);
    changetheclor(arr,n);
    for(int i=0;i<n;i++){
        cout<<arr[i];
    }

    return 0;
}

// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
void changetheclor(int arr[],int n){
    for(int i=0;i<n-1;i+=2){
        swap(arr[i],arr[i+1]);
    }
    
}

int main() {
  int arr[]={4,5,6,8,10,2};
    int n;
    n=sizeof(arr)/sizeof(arr[0]);
    changetheclor(arr,n);
    for(int i=0;i<n;i++){
        cout<<arr[i];
    }

    return 0;
}





// unique values print
# include<iostream>
using namespace std;
 void  distance(int arr[],int n){
     int i=0;
     while(i<n){
         int curr=arr[i];
         
   
 int count =0;
 // counting paramter;
while(i<n && curr==arr[i] ){
    count++;
    i++;
}
 if(count=1){
     cout<<curr;
 }
     }
 }
 int main(){
     int arr[]={4,5,5,6,8,9,4,4};
     int n=8;
    distance(arr,n);
    return 0;
}


// duplicate array
// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int arr[]={1,5,4,8,6,1,5};
    int size=sizeof(arr)/sizeof(arr[0]);
    for(int i=0;i<size;i++){
        for(int j=i+1;j<size;j++){
            if(arr[i]==arr[j]){
                cout<<"this elemet is duplciate"<<arr[i]<<endl;
            }
        }
    }
cout<<"no duplicate found"<<endl;
    return 0;
}
 
## array intersection of two arrays
// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
  
  int arr1[]={4,54,55,7};
  int arr2[]={5,4,55};
  int n1=sizeof(arr1)/sizeof(arr1[0]);
  int n2=sizeof(arr2)/sizeof(arr2[0]);
  for(int i=0;i<n1;i++){
      for(int j=1;j<n2;j++){
          if(arr1[i]==arr2[j]){
          cout<<arr2[j]<<endl;
      }
  }

  }
    return 0;
}
